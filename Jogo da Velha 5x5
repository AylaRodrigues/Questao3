#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main ()
{ 

	setlocale (LC_ALL,"");
	int quadrado[5][5];
	int player1[5][5];
	int player2[5][5];
	int vitoria = 0;
	
	//neutralizar os players
	for(int i = 0; i < 5; i++)
	{
		for(int j = 0; j < 5; j++)
		{
			player1[i][j] = 0;
			player2[i][j] = 0;
		}
	}
	//neutralizar o tabuleiro
	for(int i = 0; i < 25; i++)
	{
		for(int j = 0; j< 25; j++)
		{
			quadrado[i][j] = 0;
		}
	}
	
	return 0;	
}
